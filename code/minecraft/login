 if(password) {        var login = 'user='+username+'&password='+password+'&version=1000000';        https.request({host: 'login.minecraft.net', method: 'POST', 'Content-Type': 'application/x-www-form-urlencoded', 'Content-Length': login.length}, function(res) {            res.on('data', function(data) {                data = data.toString('utf8');                                if(data == 'Bad login\n') {                    this.emit('error', 'Bad login');                    return;                }                                data = data.split(':');                username = data[2];                sessionID = data[3];                console.log(sessionID);                                this.connect(server, username, false, sessionID);            }.bind(this));        }.bind(this)).on('error', this.emit.bind(this, 'error')).end(login);        return;    }
