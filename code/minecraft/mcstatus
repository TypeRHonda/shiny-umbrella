// Minecraft Service Status// Uses api.nomsy.net
module.exports = function (client, to, from, text, message) {    var request = require('request');    request({        url: 'http://api.nomsy.net/status'    }, function (error, response, body) {        if (!error && response.statusCode === 200) {            var service = JSON.parse(body);            var format = require("string-template");            var site = service.site == 'online' ? '\u0002\u000303\u2713\u000f' : '\u0002\u000305\u2718\u000f';            var session = service.session == 'online' ? '\u0002\u000303\u2713\u000f' : '\u0002\u000305\u2718\u000f';            var accountserver = service.accountserver == 'online' ? '\u0002\u000303\u2713\u000f' : '\u0002\u000305\u2718\u000f';            var auth = service.auth == 'online' ? '\u0002\u000303\u2713\u000f' : '\u0002\u000305\u2718\u000f';            var authserver = service.authserver == 'online' ? '\u0002\u000303\u2713\u000f' : '\u0002\u000305\u2718\u000f';            var sessionserver = service.sessionserver == 'online' ? '\u0002\u000303\u2713\u000f' : '\u0002\u000305\u2718\u000f';            var api = service.api == 'online' ? '\u0002\u000303\u2713\u000f' : '\u0002\u000305\u2718\u000f';            var returnStatement = format(                'Website: {0} Session: {1} Account Server: {2} Auth: {3} AuthServer: {4} Session Server: {5} Api: {6}',                [site, session, accountserver, auth, authserver, sessionserver, api]            );            client.say(from, to + ' - ' + returnStatement);        } else {            console.log('[Debug] Error recieving minecraft status: ' + error + ' ' + response + ' ' + body);        }    });};
