void Trigonometric_Function()  // 삼각 함수 메인 { int choice = 0; while (1) {  system("cls");  TrigonometricHeader();  printf("\t\t원하는 함수를 선택해주세요 : ");  scanf("%d", &choice);  switch (choice)  {  case 1: Sine_Function(); break;  // Sin 함수  case 2: Cosine_Function(); break;  // Cos 함수  case 3: Tangent_Function(); break; // Tan 함수  case 4: return;  default: printf("\t\t정확한 메뉴를 선택해주세요."); break;  } }}
